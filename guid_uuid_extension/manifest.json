{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "GUID Injector",
  "description": "Creates a unique GUID/UUID and inserts it into a text field",
  "version": "2.0",
  "permissions": ["contextMenus"],
  "background": {
    "service_worker": "guid_uuid.js"
  },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["tellmewhen.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "uuid.png",
    "48": "uuid_48.png",
    "16": "uuid_16.png"
  },
  "offline_enabled": true
}
